# Python program to remove duplicate elements from a list

original_list = [1, 2, 2, 3, 4, 4, 5, 1, 6]
unique_list = []
for item in original_list:
    if item not in unique_list:
        unique_list.append(item)
print("List after removing duplicates:", unique_list)

# Python program to find the GCD of two numbers
n1=int(input("enter a first number:"))
n2=int(input("enter a second number:"))
x=n1
y=n2
while(n2!=0):
    t=n2
    n2=n1%n2
    n1=t
    gcd=n1
    print("gcd of{0}and{1}={2}".format(x,y,gcd))

#Python program to reverse a string

original_string = input("Enter a string: ")
reversed_string = original_string[::-1]
print("Reversed string:", reversed_string)

# Python program to sort a list in ascending order
numbers = [5, 2, 9, 1, 5, 6]
numbers.sort()
print("Sorted list:", numbers)

# Python program to find the LCM of two numbers
n1=int(input("enter a first number:"))
n2=int(input("enter a second number:"))
x=n1
y=n2
while(n2!=0):
    t=n2
    n2=n1%n2
    n1=t
    gcd=n1
    print("gcd of{0}and{1}={2}".format(x,y,gcd))
    lcm=(x*y)/gcd
    print("lcm of{0}and{1}={2}".format(x,y,lcm))

# Python program to check if a string is a palindrome or not
text = input("Enter a string: ")
cleaned_text = text.replace(" ", "").lower()
if cleaned_text == cleaned_text[::-1]:
    print("The string is a palindrome.")
else:
    print("The string is not a palindrome.")

# Python program to find the factorial of a number
def fact(n):
 if n==0 or n==1:
    return 1
 if n>1:
    return n*fact(n-1)
# main program
num=int(input("enter a number"))
sum=0
for i in range(1,num+1):
    sum+= fact(i)
    print("sum of the factorial numbers:",sum)

#Python program to calculate the area of a triangle
l=int(input("enter a number:"))
b=int(input("enter a number:"))
A=l*b
print(A)

 # Python program to calculate the circumference of a circle
r=int(input("enter a number:"))
C=3.14*r*r
print(C)

# Python program to find the maximum of two numbers using the ternary operator
a = int(input("Enter first number: "))
b = int(input("Enter second number: "))

max_num = a if a > b else b

print("Maximum number is:", max_num)
