 # Python program to print the multiplication table of a given number
def mul(n):
  for i in range(1,11):
    print(n,'x',i,'=',n*i)
num=int(input())
mul(num)    

# Python program to find the factorial of a number using a loop
def factorial(n):
  fact=1
  for i in range(1,n+1):
    fact=fact*i
  return fact
z=int(input())
print(factorial(z))

 #Python program to find the sum of digits in a number
def sum(n):
  count=0
  for i in range(1,n+1):
    count+=i
  return count
k=int(input())
print(sum(k))

# Python program to check if a number is a palindrome or not
def is_palindrome(s):
  s=str(s)
  s=s.lower()
  return s== s[::-1]
value= input("enter a sring or number:")
if is_palindrome(value):
  print("it is palindrome")
else:
  print("it is not a palindrome")

 #Python program to find the sum of the first N natural numbers
n=int(input())
for i in range(1,n+1):
  print(i)

#Python program to find the LCM of two numbers
def gcd(a,b):
  while b!=0:
    a,b=b,a%b
  return a
def lcm(a,b):
  return (a*b)//gcd(a,b)
num1= int(input("enter a number at first:\n"))
num2= int(input("enter a number at second:\n"))
print("LCM=",lcm(num1,num2))


# the square root of a number using the Newton-Raphson method


def newton_sqrt(n, tolerance=1e-10, max_iterations=1000):
    # Handle negative numbers
    if n < 0:
        raise ValueError("Cannot compute square root of a negative number.")

    # Initial guess
    x = n

    # Newton-Raphson iteration
    for i in range(max_iterations):
        root = 0.5 * (x + n / x)
        if abs(root - x) < tolerance:
            return root
        x = root

    return x  # return last approximation


# Example usage
number = float(input("Enter a number: "))
result = newton_sqrt(number)
print(f"Square root of {number} is approximately {result}")


 # the second largest element in a list
def second_largest_sort(lst):
    unique = list(set(lst))
    if len(unique) < 2:
        raise ValueError("No second largest value.")
    unique.sort()
    return unique[-2]
print(second_largest_sort([10, 20, 40, 60, 90]))

#Python program to find the greatest common divisor (GCD) of two numbers
def gcd(n1,n2):
  while(n2!=0):
  	t=n2
  	n2=n1%n2
  	n1=t
  return n1
x=int(input())
y=int(input())
print(gcd(x,y))


# Python function to remove duplicate elements from a list
def remove_duplicates(lst):
    unique_list = []
    for item in lst:
        if item not in unique_list:
            unique_list.append(item)
    return unique_list
numbers = [1, 2, 5, 3, 1, 4, 4]
print(remove_duplicates(numbers))


